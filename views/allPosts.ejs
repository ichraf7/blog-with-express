  <section class="content">
    <!-- Basic Examples -->
    <div class="row clearfix">
      <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
        <a class="btn bg-pink waves-effect m-b-15" role="button" href="/admin/addPost" aria-expanded="false"
          aria-controls="collapseExample">
          add new post
        </a>
        <div class="card">
          <div class="header">
            <h2>
              all posts
            </h2>
          </div>
          <div class="body">
            <div class="table-responsive">
              <table class="table table-bordered table-striped table-hover js-basic-example dataTable">
                <thead>
                  <tr>
                    <th>Title</th>
                    <th>Author</th>
                    <th>category</th>
                    <th>date</th>
                    <th>views</th>
                    <th>likes</th>
                  </tr>
                </thead>
                <tfoot>
                  <tr>
                    <th>Title</th>
                    <th>Author</th>
                    <th>category</th>
                    <th>date</th>
                    <th>views</th>
                    <th>likes</th>
                  </tr>
                </tfoot>
                <tbody>
                  <%if (typeof poste!="undefined"){ %>
                  <% poste.forEach(function(posts)  { %>
                  <tr>
                    <th>
                      <%= posts.title%>
                    </th>
                    <th>
                      <%= posts.author.name%>
                    </th>
                    <th>
                      <%= posts.category.name%>
                    </th>
                    <th>
                      <%= posts.date %>
                    </th>
                    <th>
                      <%= "0"%>
                    </th>
                    <th>
                      <%= "0"%>
                    </th>

                  </tr>
                  <% })%>
                  <% } %>
                </tbody>
              </table>
            </div>
          </div>
        </div>
      </div>
    </div>
    <!-- #END# Basic Examples -->
  </section>

